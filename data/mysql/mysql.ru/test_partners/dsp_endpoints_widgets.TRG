triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `test_partners`.`dsp_endpoints_widgets_changes`\nAFTER UPDATE\nON `dsp_endpoints_widgets`\nFOR EACH ROW\nBEGIN\n\n  IF OLD.bid != NEW.bid THEN\n    INSERT INTO `test_partners_changes`.`dsp_endpoints_widgets_changes` (`endpoint_id`, `tickers_composite_id`, `banner_size`, `bid`, `geo_bids`, `subid_bids`, `blacklist_subids`, `use_predicted_cpm` )\n      VALUES (NEW.`endpoint_id`, NEW.`tickers_composite_id`, NEW.`banner_size`, NEW.bid, NEW.`geo_bids`, NEW.`subid_bids`, NEW.`blacklist_subids`, NEW.`use_predicted_cpm`);\n  END IF;\nIF OLD.geo_bids != NEW.geo_bids THEN\n    INSERT INTO `test_partners_changes`.`dsp_endpoints_widgets_changes` (`endpoint_id`, `tickers_composite_id`, `banner_size`, `bid`, `geo_bids`, `subid_bids`, `blacklist_subids`, `use_predicted_cpm` )\n      VALUES (NEW.`endpoint_id`, NEW.`tickers_composite_id`, NEW.`banner_size`, NEW.`bid`, NEW.geo_bids, NEW.`subid_bids`, NEW.`blacklist_subids`, NEW.`use_predicted_cpm`);\n  END IF;\nIF OLD.subid_bids != NEW.subid_bids THEN\n    INSERT INTO `test_partners_changes`.`dsp_endpoints_widgets_changes` (`endpoint_id`, `tickers_composite_id`, `banner_size`, `bid`, `geo_bids`, `subid_bids`, `blacklist_subids`, `use_predicted_cpm` )\n      VALUES (NEW.`endpoint_id`, NEW.`tickers_composite_id`, NEW.`banner_size`, NEW.`bid`, NEW.`geo_bids`, NEW.subid_bids, NEW.`blacklist_subids`, NEW.`use_predicted_cpm`);\n  END IF;\nIF OLD.blacklist_subids != NEW.blacklist_subids THEN\n    INSERT INTO `test_partners_changes`.`dsp_endpoints_widgets_changes` (`endpoint_id`, `tickers_composite_id`, `banner_size`, `bid`, `geo_bids`, `subid_bids`, `blacklist_subids`, `use_predicted_cpm` )\n      VALUES (NEW.`endpoint_id`, NEW.`tickers_composite_id`, NEW.`banner_size`, NEW.`bid`, NEW.`geo_bids`, NEW.`subid_bids`, NEW.blacklist_subids, NEW.`use_predicted_cpm`);\n  END IF;\nIF OLD.use_predicted_cpm != NEW.use_predicted_cpm THEN\n    INSERT INTO `test_partners_changes`.`dsp_endpoints_widgets_changes` (`endpoint_id`, `tickers_composite_id`, `banner_size`, `bid`, `geo_bids`, `subid_bids`, `blacklist_subids`, `use_predicted_cpm` )\n      VALUES (NEW.`endpoint_id`, NEW.`tickers_composite_id`, NEW.`banner_size`, NEW.`bid`, NEW.`geo_bids`, NEW.`subid_bids`, NEW.`blacklist_subids`, NEW.use_predicted_cpm);\n  END IF;\nEND'
