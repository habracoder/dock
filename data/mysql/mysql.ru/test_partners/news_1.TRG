triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `news_add` AFTER INSERT ON `news_1`\n FOR EACH ROW BEGIN \n\nINSERT INTO `news_1_statistics`(`id`) VALUES(NEW.id); \n\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `test_partners`.`news_1_changes_before` BEFORE UPDATE ON `news_1`\n FOR EACH ROW BEGIN\n\nIF OLD.droped != NEW.droped THEN\n    SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.blocked != NEW.blocked THEN\n    SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.datetime != NEW.datetime THEN\n    SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.wordnubmer != NEW.wordnubmer THEN\n    SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.animation != NEW.animation THEN\n    SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.big_photo != NEW.big_photo THEN\n    SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.newstype != NEW.newstype THEN\n    SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.is_img != NEW.is_img THEN\n    SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.title != NEW.title THEN\n  SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.description != NEW.description THEN\n  SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.social_title != NEW.social_title THEN\n  SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.social_description != NEW.social_description THEN\n  SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.is_social_img != NEW.is_social_img THEN\n  SET NEW.`updated_time` = NOW();\nEND IF;\nIF OLD.url != NEW.url THEN\n  SET NEW.`url_updated` = NOW();\n  SET NEW.`updated_time` = NOW();\nEND IF;\n\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER `news_1_delete_check` BEFORE DELETE ON `test_partners`.`news_1`\n FOR EACH ROW BEGIN\n\nDELETE FROM `news_1_statistics` WHERE `id` = OLD.id; \n\nDELETE FROM `news_1_top_statistics` WHERE `id` = OLD.id; \n\n\n\nEND'
