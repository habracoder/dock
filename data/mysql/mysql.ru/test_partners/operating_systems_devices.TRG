triggers='CREATE DEFINER=`root`@`localhost` TRIGGER `tr_AU_operating_systems_devices`\n        AFTER UPDATE\n        ON `operating_systems_devices`\n        FOR EACH ROW\nBEGIN\nIF @auth_login IS null THEN\n    INSERT INTO `test_partners_changes`.`null_who_change`(id, `who_change`) VALUES(null, @auth_login);\nEND IF;\n\nIF OLD.platform != NEW.platform THEN\n    INSERT INTO `test_partners_changes`.`operating_systems_devices_changes`(`table_id`, `who`, `field`, `old_value`, `when`, `new_value`, `mysql_user`)\n        VALUES(NEW.id, @auth_login, \'platform\', OLD.platform, NOW(), NEW.platform, USER());\nEND IF;\n\nIF OLD.platform_version != NEW.platform_version THEN\n    INSERT INTO `test_partners_changes`.`operating_systems_devices_changes`(`table_id`, `who`, `field`, `old_value`, `when`, `new_value`, `mysql_user`)\n        VALUES(NEW.id, @auth_login, \'platform_version\', OLD.platform_version, NOW(), NEW.platform_version, USER());\nEND IF;\n\nIF OLD.device_type != NEW.device_type THEN\n    INSERT INTO `test_partners_changes`.`operating_systems_devices_changes`(`table_id`, `who`, `field`, `old_value`, `when`, `new_value`, `mysql_user`)\n        VALUES(NEW.id, @auth_login, \'device_type\', OLD.device_type, NOW(), NEW.device_type, USER());\nEND IF;\n\nIF OLD.operating_systems_id != NEW.operating_systems_id THEN\n    INSERT INTO `test_partners_changes`.`operating_systems_devices_changes`(`table_id`, `who`, `field`, `old_value`, `when`, `new_value`, `mysql_user`)\n        VALUES(NEW.id, @auth_login, \'operating_systems_id\', OLD.operating_systems_id, NOW(), NEW.operating_systems_id, USER());\nEND IF;\n\nEND'
